{
    "extends": [
      "eslint:recommended",
      "react-app",
      "react-app/jest"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "es6": true
    },
    "rules": {
      "semi": ["error", "always"],
      "quotes": ["error", "single"],
      "no-console": "warn"
    }
  }